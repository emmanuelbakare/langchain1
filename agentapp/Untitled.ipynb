    "import sqlite3\n",
    "\n",
    "conn = sqlite3.connect(\"db.sqlite\")\n",
    "\n",
    "def query(query):\n",
    "    c = conn.cursor()\n",
    "    c.execute(query)\n",
    "    return c.fetchall()\n",
    "    \n",
    "    "
   ]
